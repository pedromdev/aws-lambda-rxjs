{
  "name": "aws-lambda-rxjs",
  "description": "AWS Lambda done with RxJS",
  "version": "0.0.1-alpha.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Pedro Alves <pedro.m.develop@gmail.com>",
  "homepage": "https://github.com/pedromdev/aws-lambda-rxjs#readme",
  "keywords": [
    "aws",
    "lambda",
    "rxjs"
  ],
  "scripts": {
    "prebuild": "rimraf dist coverage",
    "build": "tsc",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:cov:watch": "jest --coverage --watchAll"
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "rxjs": ">=6"
  },
  "dependencies": {
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.47",
    "@types/jest": "^25.2.1",
    "@types/rx": "^4.1.1",
    "jest": "^25.2.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
